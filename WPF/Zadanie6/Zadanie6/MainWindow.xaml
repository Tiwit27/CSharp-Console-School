<Window x:Class="Zadanie6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zadanie6"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBlock Text="DATA" Name="data" HorizontalAlignment="Center" FontSize="25"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,20,0,0">
            <Button Content="Start" Margin="0,0,200,0" Click="Button_Click"/>
            <Button Content="Czas Start" Click="Button_Click_2"/>
            <Button Content="Stop" Margin="200,0,0,0" Click="Button_Click_1"/>
        </StackPanel>
        <Image Name="image" Height="340" Source="/Grafika/img1.jpg"/>
    </StackPanel>
</Window>
