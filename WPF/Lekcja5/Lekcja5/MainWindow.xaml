<Window x:Class="Lekcja5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lekcja5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <ComboBox Name="owoce" Width="200" SelectionChanged="owoce_SelectionChanged">
            <ComboBoxItem>Jabłko</ComboBoxItem>
            <ComboBoxItem>Banan</ComboBoxItem>
            <ComboBoxItem>Pomarańcza</ComboBoxItem>
            <ComboBoxItem>Kiwi</ComboBoxItem>
        </ComboBox>
    </StackPanel>
</Window>
