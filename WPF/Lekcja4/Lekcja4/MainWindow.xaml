<Window x:Class="Lekcja4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lekcja4"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="600">
    <StackPanel Orientation="Vertical">
        <Button x:Name="buttonStackPanel" Content="Układ StackPanel" FontSize="30" Click="Button_Click"/>
        <Button x:Name="buttonWrapPanel" Content="Układ Wrap Panel" FontSize="30" Margin="0,30,0,0" Click="Button_Click"/>
        <Button x:Name="buttonDockPanel" Content="Układ DockPanel" FontSize="30" Margin="0,30,0,0" Click="Button_Click"/>
        <Button x:Name="buttonUniformGrid" Content="Układ UniformGrid" FontSize="30" Margin="0,30,0,0" Click="Button_Click"/>
        <Button x:Name="buttonGrid" Content="Układ Grid" FontSize="30" Margin="0,30,0,0" Click="Button_Click"/>
        <Button x:Name="buttonCanvas" Content="Układ Canvas" FontSize="30" Margin="0,30,0,0" Click="Button_Click"/>
    </StackPanel>
</Window>
